{
	"name": "mediawiki/semantic-forms-select",
	"type": "mediawiki-extension",
	"description": "Allows to generate a select field in Semantic Forms which values are from query",
	"keywords": [
		"MediaWiki",
		"SMW",
		"Semantic MediaWiki",
		"Semantic Forms"
	],
	"homepage": "https://www.mediawiki.org/wiki/Extension:Semantic_Forms_Select",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jasonzhang",
			"role": "Creator"
		},
		{
			"name": "Toni Hermoso Pulido",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"composer/installers": "1.*,>=1.0.1",
		"mediawiki/semantic-media-wiki": ">=2.0",
		"mediawiki/semantic-forms": "dev-master"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.3.x-dev"
		}
	},
	"autoload": {
		"classmap": [
			"vendor/SemanticForms/includes/",
			"vendor/SemanticForms/languages/",
			"vendor/SemanticForms/skins/",
			"vendor/SemanticForms/specials/",
			"vendor/SemanticForms/tests/"
		],
		"files" : [
			"vendor/SemanticForms/SemanticForms.php",
			"SemanticFormsSelect.php"
		],
		"psr-4": {
			"SFS\\": "src/"
		}
	},
	"config": {
		"process-timeout": 0
	},
	"scripts":{
		"phpunit": "php ../../tests/phpunit/phpunit.php -c phpunit.xml.dist"
	},
	"extra": {
		"merge-plugin": {
			"include": [
				"../../composer.local.json",
				"composer.json"
			],
			"merge-dev": false
		}
	}
}
